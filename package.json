{
    "name": "oauth2-workshop",
    "version": "0.1.0",
    "private": true,
    "main": "index.js",
    "dependencies": {
        "express": "^4.18.1"
    },
    "devDependencies": {
        "eslint": "^8.17.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-plugin-import": "2.26.0"
    },
    "eslintConfig": {
        "extends": "airbnb-base",
        "env": {
            "es6": true,
            "browser": true
        },
        "rules": {
            "brace-style": [
                "error",
                "stroustrup"
            ],
            "comma-dangle": [
                "error",
                "never"
            ],
            "no-unused-vars": [
                "warn"
            ],
            "no-var": [
                "off"
            ],
            "one-var": [
                "off"
            ],
            "no-console": [
                "off"
            ]
        }
    }
}